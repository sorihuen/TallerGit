Proceso UnLugar
	Definir nombre_ciudad Como Caracter;
	Definir nombre_pais Como Caracter;
	
	nombre_ciudad <- "Caracas";
	nombre_pais <- " Venezuela";
	
	Escribir " En que Ciudad Nacio? ";
